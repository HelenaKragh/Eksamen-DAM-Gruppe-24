aarhus_data2 <- aarhus_census %>%
mutate(kategori = case_when(
erhverv %in% c("Smed","Skomager") ~ "Håndværker",
erhverv%in% c("Bonde", "Jordløs Huusmand") ~ "Bonde",
erhverv%in% c("Tyende") ~ "Tjenestefolk",
famstand%in% c("Tyedee") ~ "Tjenestefolk",
TRUE ~ "Ukendt"
))
